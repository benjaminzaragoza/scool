{
  "name": "vue-test-utils-mocha-example",
  "description": "Example project using mocha-webpack and vue-test-utils",
  "version": "1.0.0",
  "author": "Evan You <yyx990803@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha-webpack --webpack-config webpack.config.js --require test/setup.js test/**/*.spec.js",
    "testwatch": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha-webpack --watch --webpack-config webpack.config.js --require test/setup.js test/**/*.spec.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-preset-stage-2": "^6.24.1",
    "moxios": "^0.4.0",
    "vue": "^2.5.17",
    "vuetify": "^1.2.5"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.19",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "expect": "^21.2.1",
    "file-loader": "^1.1.11",
    "jsdom": "^11.12.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^3.5.3",
    "mocha-webpack": "^1.1.0",
    "nyc": "^11.9.0",
    "vue-loader": "^13.7.3",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.3",
    "webpack-node-externals": "^1.7.2"
  },
  "nyc": {
    "include": [
      "src/**/*.(js|vue)"
    ],
    "instrument": false,
    "sourceMap": false
  }
}
